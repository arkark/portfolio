{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:typecheck && npm run lint:eslint && npm run lint:prettier",
    "lint:typecheck": "tsc",
    "lint:eslint": "eslint '**/*.{js,ts,jsx,tsx,json}' --ignore-path=.gitignore --max-warnings=0",
    "lint:prettier": "prettier '**/*.{js,ts,jsx,tsx,json}' --ignore-path=.gitignore --check",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "eslint '**/*.{js,ts,jsx,tsx,json}' --ignore-path=.gitignore --fix",
    "fix:prettier": "prettier '**/*.{js,ts,jsx,tsx,json}' --ignore-path=.gitignore --write"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "dayjs": "^1.11.10",
    "framer-motion": "^10.16.4",
    "next": "^12.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-scroll": "^1.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-scroll": "^1.8.10",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2"
  }
}
