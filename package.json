{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:typecheck && npm run lint:eslint && npm run lint:prettier",
    "lint:typecheck": "tsc",
    "lint:eslint": "eslint '**/*.{js,ts,jsx,tsx,json}' --ignore-path=.gitignore --max-warnings=0",
    "lint:prettier": "prettier '**/*.{js,ts,jsx,tsx,json}' --ignore-path=.gitignore --check",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "eslint '**/*.{js,ts,jsx,tsx,json}' --ignore-path=.gitignore --fix",
    "fix:prettier": "prettier '**/*.{js,ts,jsx,tsx,json}' --ignore-path=.gitignore --write"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.4.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "dayjs": "^1.11.6",
    "framer-motion": "^7.6.9",
    "next": "^12.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-scroll": "^1.8.8"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-scroll": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3"
  }
}
