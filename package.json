{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:typecheck && npm run lint:eslint && npm run lint:prettier",
    "lint:typecheck": "tsc",
    "lint:eslint": "eslint '**/*.{js,ts,jsx,tsx,json}' --ignore-path=.gitignore --max-warnings=0",
    "lint:prettier": "prettier '**/*.{js,ts,jsx,tsx,json}' --ignore-path=.gitignore --check",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "eslint '**/*.{js,ts,jsx,tsx,json}' --ignore-path=.gitignore --fix",
    "fix:prettier": "prettier '**/*.{js,ts,jsx,tsx,json}' --ignore-path=.gitignore --write"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.8",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "dayjs": "^1.11.6",
    "framer-motion": "^6.3.3",
    "next": "^12.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-scroll": "^1.8.7"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^17.0.44",
    "@types/react-scroll": "^1.8.3",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3"
  }
}
